{
  "function": {
    "prefix": "function",
    "body": "function! ${1:fn}()\n\t$2\nendfunction",
    "description": "vim function"
  },
  "if-else": {
    "prefix": "if-else",
    "body": "if ${1:condition}\n\t$2\nelseif ${3:condition}\n\t\nelse\n\t\nendif",
    "description": "if-else"
  },
  "for-loop": {
    "prefix": "for",
    "body": "for ${1:i_num} in range(1, ${2:end_num})\n\t$3\nendfor",
    "description": "for loop"
  }
}
